((egg "chickadee.egg")
 (synopsis "chicken-doc web server")
 (author "Jim Ursetto")
 (category doc-tools)
 (license "BSD")
 (doc-from-wiki)
 (needs matchable uri-common intarweb (doctype 2.0)        ; chickadee
        ;; warning: requires sendfile >=1.6.3; require 4.8 spiffy when released
        spiffy spiffy-request-vars
        html-tags html-utils 
        (chicken-doc 0.3.9) sxpath
        sxml-transforms uri-generic (chicken-doc-admin 0.3.8) colorize ; chicken-doc-html
 )
 (files "chickadee.scm" "chickadee-driver.scm"
        "chickadee.css" "chicken-doc-html.scm"
        "chickadee.setup"))
