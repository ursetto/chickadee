;;; -*- scheme -*-

((egg "chickadee.egg")
 (synopsis "chicken-doc web server")
 (author "Jim Ursetto")
 (category doc-tools)
 (license "BSD")
 (doc-from-wiki)
 (needs matchable uri-common intarweb
        (spiffy 4.8) spiffy-request-vars
        (chicken-doc 0.4.0)
        sxml-transforms uri-generic (chicken-doc-admin 0.4.0) 
        chicken-doc-html)
 (files "chickadee.scm" "chickadee-driver.scm"
        "chickadee.css" "chicken-doc-html.scm"
        "chickadee.setup"))
